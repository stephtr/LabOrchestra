{
    "root": true,
    "extends": "tonalio",
    "parserOptions": {
        "project": [
            "./tsconfig.json",
            "*/tsconfig.json"
        ]
    },
    "ignorePatterns": [],
    "rules": {
        "eslint-comments/disable-enable-pair": [
            "error",
            {
                "allowWholeFile": true
            }
        ]
    }
}